{
  "name": "shinythings",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aphrodite": "^1.2.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "react-scripts": "1.0.14",
    "@storybook/react": "^3.2.13"
  },
  "scripts": {
    "link": "rm -f node_modules/src && ln -s ../src node_modules/src",
    "start": "yarn run link && react-scripts start",
    "build": "yarn run link && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  }
}
